{
    "name": "vectorial1024/alof-lib",
    "type": "library",
    "description": "PHP array-like object functions library.",
    "keywords": [
        "library",
        "utility",
        "arrayaccess",
        "traversable",
        "arrayobject",
        "arrayiterator",
        "splobjectstorage",
        "splfixedarray",
        "weakmap"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Vectorial1024\\AlofLib\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vectorial1024\\AlofLib\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Vectorial1024"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    },
    "suggest": {
        "vectorial1024/transmutation": "A \"collections\"-like library for array-like objects."
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    }
}
